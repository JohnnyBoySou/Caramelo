{
  "expo": {
    "name": "Caramelo",
    "slug": "Caramelo",
    "version": "24.0.0",
    "scheme": "caramelo",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2D0007"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.engd.caramelo",
      "buildNumber": "24.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This application uses the camera to read barcodes to send invoices to institutions, each invoice aims to help declare taxes to the government."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#EBB000"
      },
      "package": "com.engd.caramelo",
      "versionCode": 24,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ],
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.READ_MEDIA_VIDEO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "This application uses the camera to read barcodes to send invoices to institutions.",
          "recordAudioAndroid": false
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "3e551c5c-7e5a-4239-ba02-6b9a5a1c0ba0"
      }
    }
  }
}